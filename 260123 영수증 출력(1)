package java260123_1;

class Bill {

    private String name;    // 품명
    private int price;      // 단가
    private int quantity;   // 수량

    public Bill() {}

    public Bill(String n, int p, int q) {
        name = n;
        price = p;
        quantity = q;
    }

    public String getName() {
        return name;
    }

    public int getPrice() {
        return price;
    }

    public int getQuantity() {
        return quantity;
    }
}
